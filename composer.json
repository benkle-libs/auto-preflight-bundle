        "symfony/http-kernel": ">=2.8",
        "symfony/routing": ">=2.8",
        "symfony/http-foundation": ">=2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
